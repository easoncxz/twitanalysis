{
  "name": "twitanalysis-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ru": "rollup -c",
    "mjs": "find ts-out -name '*.js' -print0 | xargs -0 -n 1 -I @ bash -c 'f=\"@\"; mv \"$f\" \"${f%.js}.mjs\"'",
    "mjs:map": "find ts-out -name '*.js.map' -print0 | xargs -0 -n 1 -I @ bash -c 'f=\"@\"; mv \"$f\" \"${f%.js.map}.mjs.map\"'",
    "build": "tsc && rollup -c",
    "dev": "concurrently --kill-others 'python3 -m http.server -d static 5000' 'tsc --watch' 'rollup -c --watch'",
    "test": "./scripts/test-module-systems.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:easoncxz/twitanalysis.git"
  },
  "keywords": [
    "twitter"
  ],
  "author": "easoncxz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/easoncxz/twitanalysis/issues"
  },
  "homepage": "https://github.com/easoncxz/twitanalysis#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "javascript-typescript-langserver": "^2.11.3",
    "rollup": "^2.26.4",
    "rollup-plugin-node-globals": "^1.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "cross-fetch": "^3.0.5",
    "crypto": "^1.0.1",
    "form-data": "^3.0.0",
    "oauth-1.0a": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
